<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="699.0" prefWidth="411.0" style="-fx-background-color: #faecc0; -fx-border-width: 5; -fx-border-color: #000000;" type="DialogPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <Label fx:id="titleLabel" alignment="CENTER" prefHeight="85.0" prefWidth="800.0" text="Available Cities">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
   </header>
   <content>
      <VBox fx:id="availableCitiesVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <ImageView fx:id="availableCity0" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="availableCity1" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="availableCity2" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
         </children>
      </VBox>
   </content>
</fx:root>
