<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="112.0" prefWidth="991.0" type="Pane" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="image" fitHeight="90.0" fitWidth="90.0" layoutX="16.0" layoutY="11.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@images/dog.jpg"/>
            </image>
        </ImageView>
        <Text fx:id="name" layoutX="122.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Billy Bob's Lobby">
            <font>
                <Font name="Satoshi Regular" size="20.0"/>
            </font>
        </Text>
        <Text fx:id="capacityText" layoutX="121.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="3/4 Players">
            <font>
                <Font name="Satoshi Regular" size="20.0"/>
            </font>
        </Text>
        <Text fx:id="expansionText" layoutX="530.0" layoutY="63.0" strokeType="OUTSIDE"
              strokeWidth="0.0" text="Orient">
            <font>
                <Font name="Satoshi Regular" size="16.0"/>
            </font>
        </Text>
        <Button fx:id="joinLobbyButton" layoutX="875.0" layoutY="33.0" mnemonicParsing="false" text="Join">
            <font>
                <Font name="Satoshi Bold" size="24.0"/>
            </font>
        </Button>
    </children>
</fx:root>
