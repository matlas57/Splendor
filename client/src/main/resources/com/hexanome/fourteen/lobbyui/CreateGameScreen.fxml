<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hexanome.fourteen.lobbyui.CreateGameScreenController">
   <children>
      <Button fx:id="backButton" layoutX="20.0" layoutY="21.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back">
         <font>
            <Font name="Satoshi Bold" size="24.0" />
         </font></Button>
      <Pane id="textOpacityPane" layoutX="58.0" layoutY="246.0" prefHeight="70.0" prefWidth="1100.0">
         <children>
            <Text layoutX="18.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Expansions: Enable an expansion pack by selecting desired packs below (none by default)">
               <font>
                  <Font name="Satoshi Italic" size="26.0" />
               </font>
            </Text>
         </children></Pane>
      <ToggleButton fx:id="selectCitiesToggle" layoutX="77.0" layoutY="331.0" mnemonicParsing="false" text="Cities">
         <font>
            <Font name="Satoshi Regular" size="24.0" />
         </font>
      </ToggleButton>
      <ToggleButton fx:id="selectTradeRoutesToggle" layoutX="206.0" layoutY="331.0" mnemonicParsing="false" text="Trade Routes">
         <font>
            <Font name="Satoshi Regular" size="24.0" />
         </font>
      </ToggleButton>
      <Button fx:id="createLobbyButton" layoutX="497.0" layoutY="596.0" mnemonicParsing="false" onAction="#handleCreateLobbyButton" text="Create Lobby">
         <font>
            <Font name="Satoshi Bold" size="26.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
